<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>WiHELP.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>WiHELP.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>approve</servlet-name>
        <servlet-class>WiHELP.approve</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>appCounselor</servlet-name>
        <servlet-class>WiHELP.appCounselor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rejCounselor</servlet-name>
        <servlet-class>WiHELP.rejCounselor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>createForum</servlet-name>
        <servlet-class>WiHELP.createForum</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>formEvent</servlet-name>
        <servlet-class>WiHELP.formEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>forumReply</servlet-name>
        <servlet-class>WiHELP.forumReply</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>testResult</servlet-name>
        <servlet-class>WiHELP.testResult</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>approve</servlet-name>
        <url-pattern>/approve</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>appCounselor</servlet-name>
        <url-pattern>/appCounselor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rejCounselor</servlet-name>
        <url-pattern>/rejCounselor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>createForum</servlet-name>
        <url-pattern>/createForum</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>formEvent</servlet-name>
        <url-pattern>/formEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>forumReply</servlet-name>
        <url-pattern>/forumReply</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testResult</servlet-name>
        <url-pattern>/testResult</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
